{
    "parserOptions": {
        "ecmaVersion": 2021,
        "sourceType": "module"
    },
    "env": {
        "browser": true,
        "node": true
    },
    "extends": [
        "plugin:vue/recommended"
    ],
    "plugins": [

    ],
    "rules": {
        "vue/max-attributes-per-line": "off",
        "vue/singleline-html-element-content-newline": "off",
        "vue/html-indent": "off",
        "vue/multi-word-component-names": "off",
        "vue/component-tags-order": "off"
    }
}