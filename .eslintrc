{
    "parserOptions": {
        "ecmaVersion": 2021,
        "sourceType": "module"
    },
    "env": {
        "browser": true,
        "node": true
    },
    "extends": [
        "plugin:vue/recommended"
    ],
    "plugins": [

    ],
    "rules": {
        "vue/max-attributes-per-line": "off",
        "vue/singleline-html-element-content-newline": "off",
        "vue/html-indent": "off",
        "vue/multi-word-component-names": "off",
        "vue/component-tags-order": "off",
        "vue/html-self-closing": "off",
        "vue/order-in-components": ["warn", {
                "order": [
                    "layout",
                    "name",
                    "components",
                    "head",
                    "props",
                    "data",
                    ["fetch", "asyncData"],
                    ["created", "beforecreated", "mounted", "beforemounted", "updated"],
                    "computed",
                    "watch",
                    "method"
                ]
            }
        ]
    }
}